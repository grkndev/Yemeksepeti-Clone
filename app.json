{
  "expo": {
    "name": "Yemeksepeti-Clone",
    "slug": "Yemeksepeti-Clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "yemeksepeti-clone",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FA0250"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FA0250",
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 1024
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
